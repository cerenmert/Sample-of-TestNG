<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Test Case Grouping">
    <test thread-count="1" name="Groups">
        <groups>
            <run>
                <include name="Barcelona"></include>
                <exclude name="Paris"></exclude>
            </run>
        </groups>
        <classes>
            <class name="com.testng.runallpackage.TestCaseGrouping"></class>
        </classes>
    </test>
</suite>